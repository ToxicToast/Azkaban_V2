{
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "targetDefaults": {
    "build": {
      "cache": true,
      "dependsOn": [
        "^build"
      ],
      "inputs": [
        "production",
        "^production"
      ]
    },
    "lint": {
      "cache": true
    },
    "e2e": {
      "cache": true
    }
  },
  "namedInputs": {
    "default": [
      "{projectRoot}/**/*",
      "sharedGlobals"
    ],
    "production": [
      "default"
    ],
    "sharedGlobals": []
  },
  "nxCloudAccessToken": "MjMwNjZiZGEtOGNlNS00ZWM4LTkzN2YtNmE3ZmMyMTc2MTY2fHJlYWQtd3JpdGU=",
  "workspaceLayout": {
    "appsDir": "apps",
    "libsDir": "libs"
  },
  "generators": {
    "@nx/node": {
      "application": {
        "projectNameAndRootFormat": "derived"
      },
      "library": {
        "projectNameAndRootFormat": "derived"
      }
    },
    "@nx/nest": {
      "application": {
        "projectNameAndRootFormat": "derived"
      },
      "library": {
        "projectNameAndRootFormat": "derived"
      }
    },
    "@nx/react": {
      "application": {
        "projectNameAndRootFormat": "derived"
      },
      "library": {
        "projectNameAndRootFormat": "derived"
      }
    },
    "@nx/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "playwright"
    },
    "@nx/angular:library": {
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nx/angular:component": {
      "style": "scss"
    }
  }
}
